<%- model_class = Service -%>
<div class="page-header">
  <h1><%= @service.name %></h1>
</div>

<div class="span3">
  <p><%= @service.description %></p>
  </div>
  <div class="span9">
  <img <%= image_tag @service.image_url.to_s %>

</div>
<br />

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              root_path, :class => 'btn'  %>
</div>
